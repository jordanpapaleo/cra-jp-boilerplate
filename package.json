{
  "name": "cra-jp-boilerplate",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "react-scripts start",
    "dev:s": "REACT_APP_SILENT=true react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "purge": "rm -rf node_modules package-lock.json; npm i"
  },
  "engines": {
    "node": "10.1.0",
    "npm": "6.0.1"
  },
  "eslintConfig": {
    "extends": [
      "standard",
      "standard-react"
    ],
    "globals": {
      "describe": true,
      "it": true,
      "xdescribe": true,
      "xit": true,
      "expect": true,
      "beforeEach": true,
      "beforeAll": true,
      "afterAll": true,
      "test": true,
      "afterEach": true,
      "React": true
    },
    "rules": {
      "react/no-unused-prop-types": [
        2,
        {
          "skipShapeProps": true
        }
      ]
    }
  },
  "standard": {
    "parser": "babel-eslint",
    "plugins": [
      "flowtype"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "dependencies": {
    "@reach/router": "1.2.1",
    "classnames": "^2.2.6",
    "flow-bin": "0.85.0",
    "lodash": "^4.17.11",
    "prop-types": "^15.6.2",
    "react": "^16.6.0",
    "react-dom": "^16.6.0",
    "react-redux": "5.1.0",
    "react-scripts": "2.1.1",
    "redux": "^4.0.1",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "styled-components": "3.4.6"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "eslint-config-standard": "^12.0.0",
    "eslint-config-standard-react": "^7.0.2",
    "eslint-plugin-flowtype": "^3.2.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^8.0.0",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-react": "^7.11.1",
    "eslint-plugin-standard": "^4.0.0",
    "source-map-explorer": "^1.6.0",
    "why-did-you-update": "^0.2.0"
  }
}
